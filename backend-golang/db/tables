CREATE TABLE IF NOT EXISTS Tokens (
    id SERIAL PRIMARY KEY ,
    token VARCHAR NOT NULL UNIQUE ,
    is_use BOOLEAN NOT NULL ,
    count INT NOT NULL
    );

